    <property name="styleSheet">
     <string notr="true">/* List Widget Styling */
QListWidget {
    background-color: #f1f7fa; /* Soft light blue background to blend with the main window */
    border: 1px solid #007BFF; /* Subtle blue border to match buttons */
    border-radius: 6px; /* Rounded corners for a modern look */
    padding: 5px; /* Padding inside the list */
    font-size: 14px; /* Consistent font size */
}

/* Regular Item Styling */
QListWidget::item {
    padding: 10px; /* Spacing around list items */
    background-color: #f9f9f9; /* Slightly lighter background for items */
    color: #333; /* Dark text for readability */
    border-bottom: 1px solid #ddd; /* Subtle bottom border between items */
}

QListWidget::item:hover {
    background-color: #e0f2f9; /* Light blue hover effect to complement the window */
    color: #007BFF; /* Blue text on hover */
}

/* Selected Item Styling */
QListWidget::item:selected {
    background-color: #007BFF; /* Solid blue for selected items */
    color: white; /* White text for contrast */
    border-radius: 4px; /* Rounded corners for selected items */
    font-weight: bold; /* Bold text for emphasis */
}

/* Alternate Row Styling */
QListWidget::item:nth-child(even) {
    background-color: #eaf4fb; /* Alternate row shading for better readability */
}

/* Scrollbar Styling for List Widget */
QScrollBar:vertical {
    border: none;
    background: #f1f7fa; /* Match the list widget background */
    width: 12px;
}

QScrollBar::handle:vertical {
    background: #007BFF; /* Blue handle to match selected items */
    border-radius: 6px; /* Rounded handle */
    min-height: 20px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    background: none;
}
</string>
